<% if vote_list %>
  <% vote_list.each do |v| %>
    <div class="row">

      <div class="span0 well text-center">
        <%= v.vote == true ? "<i class='icon-arrow-up'></i>".html_safe : "<i class='icon-arrow-down'></i>".html_safe %>
      </div>

      <div class="span8 well">
        <% if v.voteable_type == "Post" %>
          <% p = Post.find(v.voteable_id) %>
          <h4><%= link_to p.title, fix_url(p.url) %></h4>
          <p>
            <%= render "shared/poster_meta", {var_by: "posted by", var_obj: p} %>
          </p>
        <% else %>
        <% end %>     
      </div>
    </div>
  <% end %>
<% end %>
